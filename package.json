  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "pkg . --targets node18-linux-x64,node18-win-x64,node18-macos-x64 --out-path dist",
    "build:linux": "pkg . --targets node18-linux-x64 --out-path dist",
    "build:windows": "pkg . --targets node18-win-x64 --out-path dist", 
    "build:macos": "pkg . --targets node18-macos-x64 --out-path dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "pkg": "^5.8.1"
  },
  "pkg": {
    "scripts": "src/**/*.js",
    "assets": [],
    "targets": [
      "node18-linux-x64",
      "node18-win-x64", 
      "node18-macos-x64"
    ],
    "outputPath": "dist"
  },
  "bin": "src/index.js"
